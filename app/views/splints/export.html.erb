<div id='sprint_dashboard'>
  <div id='sprint_left'>
    <ul>
      <li class='sprint-left-li-right'><i class='fas fa-bars sprint-left-li-right-i'></i></li>
      <li class='sprint-left-li'><a href="/splints"><i class='fas fa-plus'></i> Add KPT</li></a>
      <li class='sprint-left-li'><a href="/splints/analysis"><i class='fas fa-chart-bar'></i> Analysis</li></a>
      <li class='sprint-left-li'><a href="/splints/link"><i class='fas fa-link'></i> Link</li></a>
      <li class='sprint-left-li'><a href="/splints/export"><i class="fas fa-file-export"></i> Export</li></a>
      <li class='sprint-left-li'><a href="/splints/project_settings"><i class='fas fa-cog'></i> Project Settings</li></a>
    </ul>
  </div>
  <div id='sprint'>
    <div id='sprint_header'>
      <p><i class="fas fa-home"></i> Project Name</p>
      <div class='flexbox'>
        <p class='project-members-plus'>+</p>
        <%= image_tag session[:picture_url]+'/small', id: 'project-members' %>
        <p class='project-members-plus'>+1</p>
        <input type="text" placeholder=' &#xf002; Search Member' class='fas project-members-search' >
        <p class='project-members-setting'><i class='fas fa-cog'></i></p>
      </div>
    </div>
    <div>
      <p>Export</p>
    </div>
  </div>
</div>